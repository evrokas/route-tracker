    <VirtualHost *:80>
        ServerName routes.yourdomain.com
        DocumentRoot ../tracker
        DirectoryIndex dashboard.php

        <Directory ../tracker>
            AllowOverride None
            Require all granted
        </Directory>

        <Directory ../tracker/data>
            Require all denied
        </Directory>

        <FilesMatch "\.(yaml|log|sqlite)$">
            Require all denied
        </FilesMatch>
    </VirtualHost>
